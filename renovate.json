{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "labels": ["dependencies"],
  "assignAutomerge": true,
  "assignees": ["Kythuen"],
  "dependencyDashboard": true,
  "prHourlyLimit": 0,
  "automerge": true,
  "timezone": "UTC+8",
  "automergeSchedule":  ["20 11 * * *"],
  "separateMinorPatch": true,
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": true,
      "automergeSchedule":  ["20 11 * * *"],
    }
  ],
  "ignoreTests": true
}
